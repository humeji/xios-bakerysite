{
  "name": "xios-bakerysite",
  "version": "13.4.9",
  "description": "Xio's Bakery - Shopify Theme (secure, production-ready)",
  "private": true,
  "scripts": {
    "test": "jest --verbose",
    "test:watch": "jest --watch --verbose",
    "test:ci": "jest --ci --coverage",
    "lint": "eslint themes/current/assets/*.js themes/development/assets/*.js tests/",
    "lint:ci": "eslint --max-warnings 0 themes/current/assets/*.js themes/development/assets/*.js tests/",
    "package": "./scripts/package-theme.sh",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/humeji/xios-bakerysite.git"
  },
  "author": "Hugo Mejia",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/humeji/xios-bakerysite/issues"
  },
  "homepage": "https://github.com/humeji/xios-bakerysite#readme",
  "devDependencies": {
    "eslint": "^10.0.2",
    "eslint-plugin-sonarjs": "^4.0.0",
    "globals": "^17.3.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jquery": "^4.0.0",
    "lint-staged": "^16.2.7"
  },
  "lint-staged": {
    "themes/*/assets/*.js": [
      "eslint --max-warnings 0"
    ],
    "tests/**/*.js": [
      "eslint --max-warnings 0"
    ]
  }
}
